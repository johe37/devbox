{
  "files.insertFinalNewline": true,
  "editor.minimap.enabled": false,
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "gitlens.advanced.messages": {
      "suppressGitMissingWarning": true
  },
  "workbench.editorAssociations": {
      "*.db": "default"
  },
  "terminal.integrated.scrollback": 10000000000,
  "editor.largeFileOptimizations": false,
  "editor.maxTokenizationLineLength": 2000000,
  "search.useIgnoreFiles": false,
  "editor.fontFamily": "MesloLGS NF",
  "settingsSync.ignoredExtensions": [
      "pkief.material-icon-theme"
  ],
  "[python]": {
      "editor.formatOnType": true,
      "editor.tabSize": 4,
      "editor.insertSpaces": true
  },
  "redhat.telemetry.enabled": false,
  "[nginx]": {
      "editor.defaultFormatter": "raynigon.nginx-formatter"
  },
  "[html]": {
      "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[ansible]": {
      "editor.detectIndentation": true,
      "editor.insertSpaces": true,
      "editor.tabSize": 2,
      "editor.quickSuggestions": {
          "comments": true,
          "other": true,
          "strings": true
      },
      "editor.autoIndent": "advanced"
  },
  "vs-kubernetes": {
    "vscode-kubernetes.minikube-path-mac": "/Users/johe/.vs-kubernetes/tools/minikube/darwin-arm64/minikube",
    "vs-kubernetes.minikube-show-information-expiration": "2024-06-04T10:38:56.017Z",
    "vs-kubernetes.crd-code-completion": "disabled"
  },
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "[javascript]": {
      "editor.tabSize": 2,
      "editor.insertSpaces": true
  },
  "[javascriptreact]": {
      "editor.tabSize": 2,
      "editor.insertSpaces": true
  }
}
